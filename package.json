{
  "name": "use-dark-theme",
  "version": "1.0.0",
  "description": "",
  "main": "lib/index.js",
  "files": [
    "lib/index.js"
  ],
  "scripts": {
    "start": "NODE_ENV=development BABEL_ENV=development node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js --config webpack.demo.js --env=dev --mode development --host 127.0.0.1 --port 3000 --progress --inline --open",
    "build": "webpack --config webpack.config.js --mode production",
    "prepare": "echo 'Building useDarkTheme' && npm run build",
    "lint": "eslint src --ext .js,.jsx --fix && eslint demo --ext .js,.jsx --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jagonzalr/useDarkTheme.git"
  },
  "keywords": [
    "React",
    "Hooks"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jagonzalr/useDarkTheme/issues"
  },
  "homepage": "https://github.com/jagonzalr/useDarkTheme#readme",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-transform-runtime"
    ]
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npm run --silent lint",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged"
    }
  },
  "peerDependencies": {
    "react": "^16.8.x"
