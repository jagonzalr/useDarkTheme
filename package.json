    "pretty-quick": "^2.0.1",
    "react": "^16.13.1",
    "react-dev-utils": "^10.2.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.4.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
